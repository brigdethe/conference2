    <script>
        document.addEventListener('DOMContentLoaded', function () {
            const teamSection = document.querySelector('.section.team');
            if (!teamSection) return;
            const speakersButton = document.getElementById('homeSpeakersButton');

            if (speakersButton) {
                speakersButton.addEventListener('click', function (event) {
                    event.preventDefault();

                    const navbar = document.getElementById('navbar');
                    const navHeight = navbar ? navbar.getBoundingClientRect().height : 0;
                    const targetY = window.pageYOffset + teamSection.getBoundingClientRect().top - navHeight - 12;

                    window.scrollTo({
                        top: Math.max(0, targetY),
                        behavior: 'smooth'
                    });
                });
            }

            const modalComponent = teamSection.querySelector('.team_modal_component');
            const modalPanel = teamSection.querySelector('.team_modal-content_wrapper');
            const modalOverlay = teamSection.querySelector('.team_modal-background_overlay');
            const modalTargets = teamSection.querySelectorAll('[cms-modal-target]');
            const modalTriggers = teamSection.querySelectorAll('[cms-modal-trigger]');

            if (!modalComponent || !modalTargets.length || !modalTriggers.length) return;

            let lockedScrollY = 0;
            let isOpen = false;
            let closeTimer = null;
            const CLOSE_ANIMATION_MS = 320;

            function lockScroll() {
                lockedScrollY = window.pageYOffset || document.documentElement.scrollTop || 0;
                document.body.style.top = '-' + lockedScrollY + 'px';
                document.body.style.position = 'fixed';
                document.body.style.width = '100%';
                document.body.style.overflow = 'hidden';
            }

            function unlockScroll() {
                document.body.style.removeProperty('top');
                document.body.style.removeProperty('position');
                document.body.style.removeProperty('width');
                document.body.style.removeProperty('overflow');
                window.scrollTo(0, lockedScrollY);
            }

            if (modalOverlay) {
                modalOverlay.style.transition = 'opacity 240ms ease';
                modalOverlay.style.opacity = '0';
            }
            if (modalPanel) {
                modalPanel.style.transition = 'transform 320ms cubic-bezier(0.22, 1, 0.36, 1), opacity 240ms ease';
                modalPanel.style.transform = 'translate3d(100%, 0, 0)';
                modalPanel.style.opacity = '0';
                modalPanel.style.willChange = 'transform, opacity';
            }
            modalComponent.style.display = 'none';
            modalComponent.style.pointerEvents = 'none';

            function closeModal() {
                if (!isOpen) return;
                isOpen = false;

                if (modalOverlay) {
                    modalOverlay.style.opacity = '0';
                }
                if (modalPanel) {
                    modalPanel.style.transform = 'translate3d(100%, 0, 0)';
                    modalPanel.style.opacity = '0';
                }

                if (closeTimer) {
                    clearTimeout(closeTimer);
                }

                closeTimer = setTimeout(function () {
                    if (isOpen) return;

                    modalTargets.forEach(function (target) {
                        target.style.display = 'none';
                    });

                    modalComponent.style.display = 'none';
                    modalComponent.style.pointerEvents = 'none';
                    unlockScroll();
                }, CLOSE_ANIMATION_MS);
            }

            function openModal(modalId) {
                const target = teamSection.querySelector("[cms-modal-target='" + modalId + "']");
                if (!target) return;

                if (closeTimer) {
                    clearTimeout(closeTimer);
                }

                modalTargets.forEach(function (item) {
                    item.style.display = 'none';
                });

                target.style.display = 'block';
                modalComponent.style.display = 'block';
                modalComponent.style.pointerEvents = 'auto';

                if (!isOpen) {
                    lockScroll();
                }
                isOpen = true;

                requestAnimationFrame(function () {
                    if (modalOverlay) {
                        modalOverlay.style.opacity = '1';
                    }
                    if (modalPanel) {
                        modalPanel.style.transform = 'translate3d(0, 0, 0)';
                        modalPanel.style.opacity = '1';
                    }
                });
            }

            modalTriggers.forEach(function (trigger) {
                trigger.addEventListener('click', function (event) {
                    event.preventDefault();
                    const modalId = trigger.getAttribute('cms-modal-trigger');
                    openModal(modalId);
                });
            });

            teamSection.querySelectorAll('[data-close-modal]').forEach(function (closeTrigger) {
                closeTrigger.addEventListener('click', function (event) {
                    event.preventDefault();
                    closeModal();
                });
            });

            document.addEventListener('keydown', function (event) {
                if (event.key === 'Escape' && isOpen) {
                    closeModal();
                }
            });
        });
    </script>
